{
  "name": "juln",
  "displayName": "juln",
  "description": "vscode全套插件，包括代码模板生成、cicd等涵盖整个开发流程",
  "version": "0.0.1",
  "publisher": "juln",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/z-juln/juln-vscode-extension.git"
  },
  "engines": {
    "vscode": "^1.68.0"
  },
  "icon": "assets/logo.png",
  "categories": [
    "Keymaps",
    "Snippets",
    "Extension Packs",
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension.js",
  "extensionPack": [
    "dbaeumer.vscode-eslint",
    "esbenp.prettier-vscode",
    "eamodio.gitlens",
    "Gruntfuggly.todo-tree",
    "wayou.vscode-todo-highlight",
    "Amour1688.ts-in-markdown",
    "alefragnani.project-manager"
  ],
  "extensionDependencies": [
    "dbaeumer.vscode-eslint",
    "esbenp.prettier-vscode"
  ],
  "contributes": {
    "configuration": [
      {
        "title": "env",
        "properties": {
          "juln.debug": {
            "title": "debug",
            "type": "boolean",
            "description": "用于插件的开发和调试",
            "default": false
          }
        }
      },
      {
        "title": "cli",
        "properties": {
          "juln.case.filename": {
            "title": "filenameCase",
            "type": "string",
            "description": "文件名规范",
            "default": "kebab",
            "enum": [
              "kebab",
              "upperCamelCase",
              "lowerCamelCase"
            ],
            "enumDescriptions": [
              "羊肉串命名法,如user-card",
              "大驼峰命名法,如UserCard",
              "小驼峰命名法,如userCard"
            ]
          }
        }
      }
    ],
    "commands": [
      {
        "command": "juln.hello",
        "title": "hello juln",
        "category": "juln"
      },
      {
        "command": "juln.createComponent",
        "title": "创建juln组件",
        "category": "juln"
      }
    ],
    "keybindings": [
      {
        "command": "juln.createComponent",
        "key": "ctrl+n",
        "mac": "cmd+n"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "juln.createComponent",
          "group": "navigation"
        }
      ],
      "editor/context": [
        {
          "command": "juln.createComponent",
          "group": "navigation"
        }
      ]
    },
    "submenus": [],
    "snippets": [
      {
        "language": "",
        "path": ""
      }
    ],
    "customEditors": [
      {
        "viewType": "",
        "displayName": "",
        "selector": [
          {
            "filenamePattern": ""
          }
        ]
      }
    ]
  },
  "scripts": {
    "build": "npx vsce package",
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out"
  },
  "keywords": [
    "juln"
  ],
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.68.0",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "@vscode/test-electron": "^2.0.3",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.6.0",
    "mocha": "^9.1.3",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4",
    "webpack": "^5.66.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "@juln/change-case": "^0.0.3",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.3",
    "super-ejs": "^0.0.6"
  }
}
